{
    "name": "mark-villudo/laravel-permission",
    "description": "Permission handling for Laravel 5.1 and up for dynamic user role permissions for laravel ",
    "keywords": [
        "markvilludo",
        "laravel",
        "permission",
        "security"
    ],
    "homepage": "https://github.com/MarkVilludo/laravel-permissions",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Villudo",
            "email": "markanthony.villudo@gmail.com",
            "role": "Back end developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/contracts": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.3.0",
        "phpunit/phpunit" : "^5.7.8|~6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "MarkVilludo\\Permission\\": "src",
            "MarkVilludo\\Permission\\": "views"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarkVilludo\\Permission\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarkVilludo\\Permission\\PermissionServiceProvider"
            ]
        }
    }
}
